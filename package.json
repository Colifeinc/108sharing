  "engines": {
    "node": "14.16.0"
  },
  "lint-staged": {
    "*.{rb,rake}": [],
    "config/webpack/**/*.js": [
      "prettier --write",
      "eslint"
    ],
    "frontend/**/*.{ts,tsx}": [
      "eslint --fix --parser-options={project:'./tsconfig-lint-staged.json'}"
    ],
    "frontend/**/*.scss": [
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "scripts": {
    "pre-commit": "lint-staged",
    "test": "jest --runInBand",
    "lint": "eslint 'frontend/**/*.{ts,tsx}'",
    "lint-fix": "eslint 'frontend/**/*.{ts,tsx}' --fix",
    "format": "prettier 'frontend/**/*.{ts,tsx}' --write",
    "icon": "npx @svgr/cli --template svgrTemplate.js --ext tsx -d frontend/icon frontend/icon"
  },
